{
  "name": "personal-data-app",
  "version": "0.1.0",
  "description": "A sample application for managing personal data",
  "repository": {
    "type": "git",
    "url": "git@github.com:mrkre/personal-data-app.git"
  },
  "author": "mrkre",
  "license": "MIT",
  "engines": {
    "node": ">=14.10",
    "yarn": ">=1.0"
  },
  "scripts": {
    "build": "yarn build-ts && yarn lint",
    "build-ts": "tsc",
    "debug": "yarn build && yarn watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "yarn serve",
    "test": "NODE_ENV=test jest --forceExit --coverage --verbose",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "yarn test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "async": "^3.1.0",
    "bcrypt": "^5.0.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "lusca": "^1.6.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.7.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.1.17",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/async": "^3.0.2",
    "@types/bcrypt": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.3",
    "@types/compression": "^1.0.1",
    "@types/concurrently": "^4.1.0",
    "@types/connect-mongo": "^3.0.0",
    "@types/cryptr": "^4.0.1",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/faker": "^5.1.2",
    "@types/http-status": "^1.1.2",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.141",
    "@types/lusca": "^1.6.1",
    "@types/mongoose": "^5.5.18",
    "@types/node": "^12.7.8",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.1.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "jest": "^26.0.14",
    "jest-environment-node": "^26.3.0",
    "nodemon": "^1.19.2",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
